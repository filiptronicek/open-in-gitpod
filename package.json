{
  "name": "open-in-gitpod",
  "license": "MIT",
  "displayName": "Open in Gitpod",
  "publisher": "filiptronicek",
  "repository": "https://github.com/filiptronicek/vs-code-gitpod/",
  "description": "An extension for opening Gitpod from your current workspace",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:open-in-gitpod.open",
    "onCommand:open-in-gitpod.copy"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "open-in-gitpod.open",
        "category": "Open in Gitpod",
        "title": "Open"
      },
      {
        "command": "open-in-gitpod.copy",
        "category": "Open in Gitpod",
        "title": "Copy Gitpod URL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}
